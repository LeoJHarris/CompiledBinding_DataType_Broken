        TargetType="Button" />

    <Style
        x:Key="SmallPrimaryButtonStyle"
        BasedOn="{StaticResource BaseSmallPrimaryButtonStyle}"
        TargetType="Button" />

    <Style
        x:Key="NegativeGhostButtonStyle"
        BasedOn="{StaticResource BaseNegativeGhostButtonStyle}"
        TargetType="Button" />

    <Style
        x:Key="SwitchStyle"
        BasedOn="{StaticResource BaseSwitchStyle}"
        TargetType="Switch" />

    <Style
        x:Key="TimePickerStyle"
        BasedOn="{StaticResource BaseTimePickerStyle}"
        TargetType="TimePicker" />

    <Style
        x:Key="PickerStyle"
        BasedOn="{StaticResource BasePickerStyle}"
        TargetType="Picker" />

    <Style
        x:Key="DatePickerStyle"
        BasedOn="{StaticResource BaseDatePickerStyle}"
