        BasedOn="{StaticResource BaseActivityIndicatorSmallStyle}"
        TargetType="ActivityIndicator" />

    <x:String x:Key="LogoVariant1ImageSource">SampleApp.Resources.Images.Methsof.logo.png</x:String>
    <x:String x:Key="LogoVariant2ImageSource">SampleApp.Resources.Images.Methsof.logo_inverted.png</x:String>
    <x:String x:Key="ConversationImageSource">SampleApp.Resources.Images.Methsof.logo_transparent.png</x:String>
    <x:String x:Key="InstantConsultImageSource">SampleApp.Resources.Images.CWHAU.instant_consult_icon.png</x:String>

    <x:String x:Key="AppBrandName">Methsof App</x:String>
